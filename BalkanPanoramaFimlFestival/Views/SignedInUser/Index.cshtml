
@{
    ViewData["Title"] = "Index";
}

<h1>Hoşgeldin @User.Identity!.Name </h1>

<div class="row">
    <div class="col-md-6 offset-md-3">
        <h1>Kullanıcı Bilgileri</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>Kullanıcı adı:</th>
                    <th>Kullanıcı soyadı:</th>
                    <th>Email:</th>
                    <th>Telefon No:</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th>@Model.FirstName</th>
                    <th>@Model.LastName</th>
                    <th>@Model.Email</th>
                    <th>@Model.PhoneNumber</th>
                </tr>
            </tbody>
        </table>
    </div>
</div>

